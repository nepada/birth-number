{
    "name": "nepada/birth-number",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Czech birth number value object.",
    "keywords": ["birth number", "value object", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/birth-number/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.4",
        "nette/utils": "^3.2@dev || ^4.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.2",
        "phpstan/phpstan": "1.11.3",
        "phpstan/phpstan-strict-rules": "1.6.0",
        "phpstan/phpstan-nette": "1.3.0",
        "nepada/phpstan-nette-tester": "1.2.0",
        "spaze/phpstan-disallowed-calls": "3.4.0",
        "shipmonk/phpstan-rules": "3.0.0",
        "php-parallel-lint/php-parallel-lint":  "1.4.0",
        "nepada/coding-standard": "7.14.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\BirthNumber\\": "src/BirthNumber/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
